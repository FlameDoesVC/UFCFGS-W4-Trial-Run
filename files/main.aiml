<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <category>
        <pattern>AIMLLOCATION *</pattern>
        <template>
            <think><set var="checklocations"><map name="locations"><star/></map></set></think>
            <condition var="checklocations">
                <li value="unknown"><think><set name="error">nocity</set></think>I do not know where <star/> is</li>
                <li><think><set name="error">noerror</set></think><get var="checklocations" /></li>
            </condition>
        </template>
    </category>
    
    <category>
        <pattern>Why not</pattern>
        <template>
            <think><set var="checkerror"><get name="error" /></set></think>
            <condition var="checkerror">
                <li value="nocity">Because I do not have a category to connect me to a mapping service</li>
                <li value="population">Because it is too hard to count the residents of <get name="lastaskedlocation" /></li>
                <li><srai>oogaboogaerrorohnobooooooooo</srai></li>
            </condition>
        </template>
    </category>
    
    <category>
        <pattern>Where is *</pattern>
        <template>
            <think><set var="checkit"><star/></set></think>
            <condition var="checkit">
                <li value="it"><srai>AIMLLOCATION <get name="lastaskedlocation" /></srai></li>
                <li><srai>AIMLLOCATION <star/></srai></li>
            </condition>
        </template>
    </category>
    
    <category>
        <pattern>_ where * _</pattern>
        <template>
            <srai>Where is <star index="2"/></srai>
        </template>
    </category>
    
    <category>
        <pattern>_ where is * _</pattern>
        <template>
            <srai>Where is <star index="2"/></srai>
        </template>
    </category>
    
    <category>
        <pattern>Whereabouts is *</pattern>
        <template>
            <srai>Where is <star/></srai>
        </template>
    </category>
    
    <category>
        <pattern>Tell me where * is</pattern>
        <template>
            <srai>Where is <star/></srai>
        </template>
    </category>
    
    <category>
        <pattern>Where is * _</pattern>
        <template>
            <srai>Where is <star/></srai>
        </template>
    </category>
    
    <category>
        <pattern>What is *</pattern>
        <template>
            <think><set name="error">noerror</set></think>
            <think><set var="checklocations"><map name="locations"><star/></map></set></think>
            <condition var="checklocations">
                <li value="unknown">I do not have an answer for that</li>
                <li value="In the eye of the beholder">A subjective judgment evoked by an emotional response</li>
                <li value="In north Bristol"><srai>ohnoyetanothererrorwhoopsies</srai></li>
                <li><set name="lastaskedlocation"><star/></set> is a city.</li>
            </condition>
        </template>
    </category>
    
    <category>
        <pattern>what is the population of *</pattern>
        <template>
            <think><set var="checkpopulations"><map name="populations"><star/></map></set></think>
            <condition var="checkpopulations">
                <li value="unknown"><think><set name="error">population</set><set name="lastaskedlocation"><star/></set></think>I do not know where <star/> is</li>
                <li><think><set name="error">noerror</set></think>The population of <star/> is about <get var="checkpopulations" /></li>
            </condition>
        </template>
    </category>
    
    <category>
        <pattern>How many people live in *</pattern>
        <template><srai>what is the population of <star/></srai></template>
    </category>
</aiml>